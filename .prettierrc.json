{
  "trailingComma": "es5",
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "bracketSpacing": true,
  "overrides": [
    {
      "files": ["*.json", ".*"],
      "options": {
        "useTabs": false
      }
    },
    {
      "files": ["*.html"],
      "options": {
        "printWidth": 120
      }
    }
  ]
}
